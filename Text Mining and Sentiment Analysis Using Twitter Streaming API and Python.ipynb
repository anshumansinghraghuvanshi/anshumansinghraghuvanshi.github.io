    "\n",
    "# Plot histogram\n",
    "ax = sns.barplot(cd,[obama_pol, trump_pol, putin_pol, modi_pol])\n",
    "ax.set(ylabel=\"Polarity Mean\")\n",
    "plt.show()"
   ]
